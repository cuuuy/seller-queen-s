				},            // Json 형식의 데이터이다.
			success : function(res){ // 비동기통신의 성공일경우 success콜백으로 들어옵니다. 'res'는 응답받은 데이터이다.
				// 응답코드 > 0000
				console.log(res);
				
				for(var i = 0; i < res.length; i++){
					
					if(res[i].isShow=="YES"){
						var isShow = "판매중";
					}


					var unix_timestamp = res[i].enroll_time;
					var date = new Date(unix_timestamp * 1000);
					var hours = date.getHours();
					var minutes = "0" + date.getMinutes();
					var seconds = "0" + date.getSeconds();
					var mon = (date.getMonth()+1);
					var formattedTime = date.getFullYear() + "-" + mon + "-" + date.getDate() + " " + hours + ':' + minutes.substr(-2) + ':' + seconds.substr(-2);
					var img_path = res[i].main_img_url.replace("public","");

					var new_html = `<tr>
										<td>
											<div class="adminContent_table_checkbox">
												<input id="check06" type="checkbox">
												<label for="check06"><span></span></label>
											</div>
										</td>
										<td>`+res[i].id+`</td>
										<td class="adminContent_table_name">
											<span class="adminContent_table_name_thumb" style="background: url(http://43.200.169.89`+img_path+`) no-repeat; background-size:cover;"></span>
											<p>`+res[i].product_name+`</p>
										</td>
										<td>`+res[i].wholesaler+`</td>
										<td>`+res[i].category_id+`</td>
										<td>`+res[i].retail_price+`원</td>
										<td>`+isShow+`</td>
										<td>`+res[i].remain_count+`</td>
										<td>`+formattedTime+`</td>
										<td>`+res[i].delivery_fee+`원</td>
										<td>
											<label class="switch">
												<input type="checkbox">
												<span id="product_show" class="slider" onclick="set_product_showed(`+res[i].id+`);"></span>
												<span id="product_hide" class="slider" onclick="set_product_not_showed(`+res[i].id+`);" style="display:none;"></span>
											</label>
										</td>
										<td>
											<label class="switch">
												<input type="checkbox">
												<span id="best_product_show" class="slider" onclick="set_best_product_showed(`+res[i].id+`);"></span>
												<span id="best_product_hide" class="slider" onclick="set_best_product_not_showed(`+res[i].id+`);" style="display:none;"></span>
											</label>
										</td>
									</tr>`;

					$("#product_road_tbody").append(new_html);
				}

			},
			error : function(XMLHttpRequest, textStatus, errorThrown){ // 비동기 통신이 실패할경우 error 콜백으로 들어옵니다.
				alert("통신 실패.")
			}
		});
	}
